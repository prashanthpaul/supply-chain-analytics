            <column-instance column='[units_sold]'   derivation='Sum'  name='[sum:units_sold:ok]' pivot='value' type='quantitative' />
            <column-instance column='[category]'     derivation='None' name='[none:category:nk]'  pivot='key'   type='nominal' />
